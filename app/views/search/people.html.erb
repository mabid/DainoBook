<div class='notice_blue' id="forms-login" style="width:700px;">
<div class='notice_head' id="right-pane-head">
<div style="float: left; vertical-align: bottom; padding-top: 9px; padding-right: 8px;">
</div>
<div class='notice_body'>
	<% @users.each do |user| %>
  <div style="width:200;float:left;">
<%= image_tag user.profile_picture.url(:small) %>
<div><strong><%= user.full_name %> </strong></div>
<% unless user == current_user || current_user.requested_friends.include?(user) || current_user.friends.include?(user) || current_user.pending_friends.include?(user) %>
<% form_for(:friendship, :url => user_friends_path(:user_id => current_user.id, :friend_id => user.id)) do |f| %>
<%= submit_tag "Add to My Friends" %>
<% end %>
<% end %>
</div>
	<% end %>
</div>

</div>
</div>
</div>
