<div class='notice_blue' id="forms-login" style="width:700px;">
	<div class='notice_head' id="right-pane-head">
          <div align="left"><%= link_to "Back to albums", :user_photos %></div>
         <div style="float: left; vertical-align: bottom; padding-top: 9px; padding-right: 8px;">
	<strong>Album Name : <%= @album.name %></strong>
	<strong>Location : <%= @album.location %></strong>
<a href="<%= new_user_album_photo_path(current_user, @album) %>">	<strong>Upload</strong></a>
	<div class='notice_body'>
	<% @album.photos.each do |photo| %>
	<div style="width:200;float:left; margin: 1px;">
<a href="<%= user_album_photo_path(current_user, @album.id, photo.id) %>"	><%= image_tag photo.picture.url(:small), :class=>'album_list'%> </a>
	<div align="center"><strong><%= photo.caption %> </strong></div>
	</div>
	<% end %>
	</div>

	</div>
	</div>
</div>
